{
    "name": "narekps/ZfbUser",
    "description": "A simple user authentication module for Zend Framework",
    "type": "library",
    "license": "MIT License",
    "keywords": [
        "zf",
        "user",
        "authentication"
    ],
    "homepage": "https://github.com/narekps/ZfbUser",
    "require": {
        "php": "^7.1",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-mvc-plugins": "^1.0",
        "zendframework/zend-mail": "^2.8",
        "zendframework/zend-crypt": "^3.2",
        "zendframework/zend-captcha": "^2.7",
        "zendframework/zendservice-recaptcha": "^3.0",
        "doctrine/doctrine-module": "^1.2",
        "doctrine/doctrine-orm-module": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "ZfbUser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfbUserTest\\": "test/"
        }
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "^1.1.0",
        "zendframework/zend-test": "^3.0.1",
        "san/san-session-toolbar": "^1.0"
    }
}
